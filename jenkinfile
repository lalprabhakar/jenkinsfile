pipeline{
    agent any
    stages{
        stage('Git Clone'){
            steps {
                sh 'rm -rf pipeline_git*; git clone git@github.com:opstree/spring3hibernate.git'
            }
         }
        
    
          stage('Code Stability'){
            steps {
                sh ' cd spring3hibernate; mvn compile'
                
            }
    }
    }
}

